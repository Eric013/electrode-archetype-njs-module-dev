{
  "name": "@walmart/electrode-archetype-hapi-plugin-dev",
  "version": "2.0.2",
  "description": "Builder Archetype - Hapi Plugin (Development)",
  "scripts": {
    "lint-lib": "eslint -c node_modules/@walmart/electrode-archetype-hapi-plugin-dev/config/eslint/.eslintrc-node lib --color",
    "lint-test": "eslint -c node_modules/@walmart/electrode-archetype-hapi-plugin-dev/config/eslint/.eslintrc-test test --color",
    "lint": "builder concurrent lint-lib lint-test",
    "test": "builder concurrent lint-lib lint-test test-only",
    "test-only": "mocha -c --opts node_modules/@walmart/electrode-archetype-hapi-plugin-dev/config/test/mocha.opts test/spec",
    "test-cov": "istanbul cover _mocha -- -c --opts node_modules/@walmart/electrode-archetype-hapi-plugin-dev/config/test/mocha.opts test/spec",
    "test-ci": "builder run test-cov",
    "check": "builder run lint && builder run test-cov",
    "check-ci": "builder run lint && builder run test-ci",
    "builder:lint-server": "eslint --color -c config/eslint/.eslintrc-test config/test",
    "builder:lint": "npm run builder:lint-server",
    "builder:check": "npm run builder:lint",
    "builder:prepublish": "builder-support gen-dev"
  },
  "repository": {
    "type": "git",
    "url": "git@gecgithub01.walmart.com:electrode/electrode-archetype-hapi-plugin-dev.git"
  },
  "author": "Chase Adams",
  "license": "MIT",
  "dependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^8.0.1",
    "eslint-plugin-filenames": "^0.2.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "devDependencies": {
    "builder-support": "0.0.1"
  }
}
